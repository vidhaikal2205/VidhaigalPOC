<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mobile_Number_Format</fullName>
    <active>false</active>
    <description>Registrant need to enter a mobile number valid of 10 digits</description>
    <errorConditionFormula>NOT(REGEX( Mobile_Number__c , &quot;^[0-9]{10}$&quot;))</errorConditionFormula>
    <errorMessage>Enter a valid mobile number</errorMessage>
</ValidationRule>
